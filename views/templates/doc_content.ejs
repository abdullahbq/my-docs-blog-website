<%- include('../partials/head') %>
    <%- include('../partials/header') %>
        <div class="container mt-4">
            <div class="row">
                <div class="col-md-3">
                    <div class="list-group">
                        <% docs.forEach(doc=> { %>
                            <a href="/documentation/<%= doc.replace('.md', '') %>"
                                class="list-group-item list-group-item-action">
                                <%= doc.replace('.md', '' ) %>
                            </a>
                            <% }) %>
                    </div>
                </div>
                <div class="col-md-9">
                    <h1>
                        <%= title %>
                    </h1>
                    <div><%- content %></div>
                </div>
            </div>
        </div>
        <%- include('../partials/footer') %>